<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <navigation-rule>
  <from-view-id>/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>admin</from-outcome>
   <to-view-id>/adminPage.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>HREuser</from-outcome>
   <to-view-id>/HREuserPage.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>FLEuser</from-outcome>
   <to-view-id>/FLEuserPage.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/adminPage.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>addHourly</from-outcome>
   <to-view-id>/addHourly.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>addFlat</from-outcome>
   <to-view-id>/addFlat.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>FLE</from-outcome>
   <to-view-id>/modifyFlat.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>HRE</from-outcome>
   <to-view-id>/modifyHourly.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/addHourly.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/adminPage.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/addFlat.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/adminPage.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/modifyHourly.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/adminPage.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/modifyFlat.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/adminPage.xhtml</to-view-id>
  </navigation-case>
